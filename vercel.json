{
  "framework": "sveltekit",
  "functions": {
    "src/routes/api/assets/[...path]/+server.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/assets/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, HEAD, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Range"
        }
      ]
    }
  ]
} 